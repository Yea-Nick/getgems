    "/minting/{collectionAddress}/update/{nftAddress}": {
        "get": {
            "tags": [
                "MintingApi"
            ],
            "operationId": "getUpdateNftStatus",
            "parameters": [
                {
                    "in": "path",
                    "required": true,
                    "name": "collectionAddress",
                    "schema": {
                        "type": "string",
